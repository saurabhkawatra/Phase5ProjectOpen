pipeline {
    agent any 
    stages {
        stage('compile and clean') { 
            steps {
		sh "mvn clean compile"
            }
        }
                stage('Deploy') { 
            steps {
                sh "mvn package "
            }
		stage('Execution') { 
            steps {
                sh "java -jar Phase5ProjectOpen.jar"
            }
        }
    }
}
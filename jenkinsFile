pipeline {
    agent any 
    stages {
        stage('compile and clean') { 
            steps {
		sh "mvn clean compile"
            }
        }
                stage('Deploy') { 
            steps {
                sh "mvn clean install"
            } 
	}
		stage('Execution') { 
            steps {
		sh "cd 'target'"
                sh "java -jar Phase5ProjectOpen.jar"
            }
	}

    }
}